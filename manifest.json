{
  "manifest_version": 2,
  "name": "Gudfood Randomizer",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": ["http://orders.gudfood.com.ua/order"],
      "js": ["content.js"]
    }
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_title": "Randomize Menu"
  }
}
